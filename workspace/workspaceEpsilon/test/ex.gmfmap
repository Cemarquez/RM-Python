<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="ex.ecore#//Filesystem/drives"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="ex.ecore#//Drive"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="ex.gmfgraph#DriveLabel"/>
        <features
            href="ex.ecore#//File/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="ex.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="ex.gmfgraph#Drive"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0"
          referencedChild="//@nodes.0/@ownedChild">
        <containmentFeature
            href="ex.ecore#//Folder/contents"/>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="ex.ecore#//Folder/contents"/>
        <ownedChild
            relatedDiagrams="//@diagram">
          <domainMetaElement
              href="ex.ecore#//Folder"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="ex.gmfgraph#FolderLabel"/>
            <features
                href="ex.ecore#//File/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="ex.gmftool#//@palette/@tools.0/@tools.2"/>
          <diagramNode
              href="ex.gmfgraph#Folder"/>
          <children
              compartment="//@nodes.0/@ownedChild/@children.1/@ownedChild/@compartments.0"
              referencedChild="//@nodes.0/@ownedChild">
            <containmentFeature
                href="ex.ecore#//Folder/contents"/>
          </children>
          <children
              compartment="//@nodes.0/@ownedChild/@children.1/@ownedChild/@compartments.0"
              referencedChild="//@nodes.0/@ownedChild/@children.1/@ownedChild">
            <containmentFeature
                href="ex.ecore#//Folder/contents"/>
          </children>
          <children
              compartment="//@nodes.0/@ownedChild/@children.1/@ownedChild/@compartments.0"
              referencedChild="//@nodes.0/@ownedChild/@children.2/@ownedChild">
            <containmentFeature
                href="ex.ecore#//Folder/contents"/>
          </children>
          <children
              compartment="//@nodes.0/@ownedChild/@children.1/@ownedChild/@compartments.0"
              referencedChild="//@nodes.0/@ownedChild/@children.3/@ownedChild">
            <containmentFeature
                href="ex.ecore#//Folder/contents"/>
          </children>
          <compartments
              children="//@nodes.0/@ownedChild/@children.1/@ownedChild/@children.0 //@nodes.0/@ownedChild/@children.1/@ownedChild/@children.1 //@nodes.0/@ownedChild/@children.1/@ownedChild/@children.2 //@nodes.0/@ownedChild/@children.1/@ownedChild/@children.3">
            <compartment
                href="ex.gmfgraph#FolderContentsCompartment"/>
          </compartments>
        </ownedChild>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="ex.ecore#//Folder/contents"/>
        <ownedChild>
          <domainMetaElement
              href="ex.ecore#//Shortcut"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="ex.gmfgraph#ShortcutLabel"/>
            <features
                href="ex.ecore#//File/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="ex.gmftool#//@palette/@tools.0/@tools.3"/>
          <diagramNode
              href="ex.gmfgraph#Shortcut"/>
        </ownedChild>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="ex.ecore#//Folder/contents"/>
        <ownedChild>
          <domainMetaElement
              href="ex.ecore#//File"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="ex.gmfgraph#FileLabel"/>
            <features
                href="ex.ecore#//File/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="ex.gmftool#//@palette/@tools.0/@tools.1"/>
          <diagramNode
              href="ex.gmfgraph#File"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0 //@nodes.0/@ownedChild/@children.1 //@nodes.0/@ownedChild/@children.2 //@nodes.0/@ownedChild/@children.3">
        <compartment
            href="ex.gmfgraph#DriveContentsCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="ex.ecore#//Sync"/>
    <containmentFeature
        href="ex.ecore#//Filesystem/syncs"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="ex.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="ex.gmfgraph#Sync"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="ex.ecore#//Sync/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="ex.ecore#//Sync/target"/>
  </links>
  <links>
    <labelMappings
        xsi:type="gmfmap:DesignLabelMapping"
        readOnly="true">
      <diagramLabel
          href="ex.gmfgraph#ShortcutTargetExternalLabel"/>
    </labelMappings>
    <tool
        xsi:type="gmftool:CreationTool"
        href="ex.gmftool#//@palette/@tools.1/@tools.1"/>
    <diagramLink
        href="ex.gmfgraph#ShortcutTarget"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="ex.ecore#//Shortcut/target"/>
  </links>
  <diagram>
    <diagramCanvas
        href="ex.gmfgraph#filesystem"/>
    <domainModel
        href="ex.ecore#/"/>
    <domainMetaElement
        href="ex.ecore#//Filesystem"/>
    <palette
        href="ex.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
