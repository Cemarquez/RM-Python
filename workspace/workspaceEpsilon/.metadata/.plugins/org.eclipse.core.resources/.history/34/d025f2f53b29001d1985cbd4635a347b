@namespace(uri="abstract", prefix="abstract")
@gmf
package abstracts;

class MClassDiagram{
	val MPackage;
	val MClass;
}

class MPackage{
	attr String name;
	attr String path;
	val MClass;
}

class MClass{
	attr String name;
	attr String accessModifier;
	attr String comments;
	attr boolean abstracts;
	attr String stereoType;
	attr String path;
	val MRelationship;
}

class MRelationship{
	attr String relationshipType;
	attr String multiplicitySource;
	attr String multiplicityTarget;
	attr 
}

